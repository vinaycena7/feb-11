sno=int(input('enter student number:'))

sname=input('enter student name:')

sgroup=input('enter student group:')

s1=int(input('enter computer science marks:'))

s2=int(input('enter  maths marks:'))

s3=int(input('enter  chemistry marks:'))

s4=int(input('enter  telugu marks:'))

s5=int(input('enter  english marks:'))

s6=int(input('enter  analytical skills :'))

total= s1 + s2 + s3 + s4 + s5 + s6
 
avg=total/6
 
if avg>=91:
     print('O grade')
elif avg>=81:
     print('A grade')
elif avg>=71:
     print('B grade')
elif avg>=61:
     print('C grade')
elif avg>=51:
     print('D grade')
else:
     print('fail')
output:
enter student number:49
enter student name:vetcha.vinsy
enter student group:mccs
enter  computer science marks:96
enter  maths marks:98
enter  chemistry marks:97
enter  telugu marks:95
enter  english marks:97
enter  analytical skills :98
O grade
